compile_and_run_p1       $1 test_suite -O3 -lcx -C COMPACT

compile_and_run_p1       $1 test_suite -O4 -lcx 
compile_and_run_p1       $1 test_suite -O4 -lcx -C COMPACT

compile_and_run_p1       $1 test_suite -O5 -lcx 
compile_and_run_p1       $1 test_suite -O5 -lcx -C COMPACT

compile_and_run_p1       $1 test_suite -O1 -lcix 
compile_and_run_p1       $1 test_suite -O1 -lcix -C COMPACT

compile_and_run_p1       $1 test_suite -O2 -lcix 
compile_and_run_p1       $1 test_suite -O2 -lcix -C COMPACT

compile_and_run_p1       $1 test_suite -O3 -lcix 
compile_and_run_p1       $1 test_suite -O3 -lcix -C COMPACT

compile_and_run_p1       $1 test_suite -O4 -lcix 
compile_and_run_p1       $1 test_suite -O4 -lcix -C COMPACT

compile_and_run_p1       $1 test_suite -O5 -lcix 
compile_and_run_p1       $1 test_suite -O5 -lcix -C COMPACT



compile_and_run_p1       $1 test_multiple_cogs -ltiny -lc -lm 
