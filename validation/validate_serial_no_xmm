compile_and_run_p1       $1 test_threads -lthreads -O1 -lcx -lm 
compile_and_run_p1       $1 test_threads -lthreads -O1 -lcx -lm -C COMPACT

compile_and_run_p1       $1 test_threads -lthreads -O2 -lcx -lm 
