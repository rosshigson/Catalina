compile_and_run_p1       $1 test_float -O3 -lc -lma -C COMPACT

compile_and_run_p1       $1 test_float -O4 -lc -lma -C COMPACT

compile_and_run_p1       $1 test_float -O5 -lc -lma -C COMPACT

compile_and_run_p1       $1 test_float -lc -lmb -C COMPACT

