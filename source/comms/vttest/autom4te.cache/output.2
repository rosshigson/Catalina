${GROFF_NOTE}
${GROFF_NOTE}.1.ps :
${GROFF_NOTE}	\$(SHELL) -c "tbl \$*.1 | groff -man" >\$@
${GROFF_NOTE}
${GROFF_NOTE}.1.txt :
